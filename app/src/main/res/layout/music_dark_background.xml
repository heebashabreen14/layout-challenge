<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#222222"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/a"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.5"
        android:background="#00000"
        android:text="Music"
        android:textColor="#FFFFFF"
        android:textSize="60dp"
        android:padding="15dp"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/b"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00000"
        android:gravity="center"
        android:text="Favorite"
        android:textFontWeight="1"
        android:layout_margin="10dp"
        android:textColor="#818181"
        app:layout_constraintTop_toBottomOf="@+id/a"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/c"
        android:textSize="20dp"
        />
    <TextView
        android:id="@+id/c"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00000"
        android:text="Popular"
        android:textColor="#818181"
        android:gravity="center"
        android:textFontWeight="1"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/a"
        app:layout_constraintStart_toEndOf="@+id/b"
        app:layout_constraintEnd_toStartOf="@+id/d"

        android:textSize="20dp"
        />
    <TextView
        android:id="@+id/d"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:background="#00000"
        android:text="Albums"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:textFontWeight="1"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/a"
        app:layout_constraintStart_toEndOf="@+id/c"
        app:layout_constraintEnd_toStartOf="@+id/e"
        android:textSize="20dp"

        />
    <TextView
        android:id="@+id/e"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00000"
        android:text="Latest"
        android:textColor="#818181"
        android:gravity="center"
        android:textFontWeight="1"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/a"
        app:layout_constraintStart_toEndOf="@+id/d"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="20dp"
        />
    <TextView
        android:id="@+id/f"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00000"
        android:text="Download"
        android:textColor="#DCDCDC"
        android:gravity="center"
        android:textFontWeight="1"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/b"
        app:layout_constraintStart_toStartOf="parent"

        android:textSize="20dp"
        />
    <TextView
        android:id="@+id/g"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00000"
        android:text="6 Albums"
        android:textColor="#DCDCDC"
        android:gravity="center"
        android:textFontWeight="1"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@+id/e"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="20dp"
        />



<!--        />-->
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#707070"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="@+id/b"
        app:layout_constraintBottom_toTopOf="@+id/f"
        android:layout_marginTop="20dp"
        />
    <TextView
        android:id="@+id/h"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Supernova"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/f"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        />
    <TextView
        android:id="@+id/i"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kat Cunning"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/h"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="#818181"
        android:textSize="20dp"
        android:layout_marginStart="10dp"
        />

    <TextView
        android:id="@+id/j"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Candy"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/h"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        />
    <TextView
        android:id="@+id/k"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Robbie Williams"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/j"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="#818181"
        android:textSize="20dp"
        android:layout_marginStart="10dp"
        />
    <TextView
        android:id="@+id/l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Remember the Name"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/k"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        />
    <TextView
        android:id="@+id/m"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fort Minor,Styles Of Beyond"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/l"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="#818181"
        android:textSize="20dp"
        android:layout_marginStart="10dp"
        />
    <TextView
        android:id="@+id/n"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tightrope"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/m"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        />
    <TextView
        android:id="@+id/o"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ZAYN"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/n"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="#818181"
        android:textSize="20dp"
        android:layout_marginStart="10dp"
        />
    <TextView
        android:id="@+id/p"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Without Me"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/o"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        />
    <TextView
        android:id="@+id/q"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Halsey"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/p"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="#818181"
        android:textSize="20dp"
        android:layout_marginStart="10dp"
        />
    <TextView
        android:id="@+id/r"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:background="#004740"
        android:gravity="center"
        android:text="Home"
        android:textFontWeight="1"
        android:textStyle="bold"
        android:textColor="#96DCD5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/s"

        android:textSize="20dp"
        />
    <TextView
        android:id="@+id/s"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:background="#004740"
        android:gravity="center"
        android:text="Library"
        android:textFontWeight="1"
        android:textStyle="bold"
        android:textColor="#96DCD5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/r"
        app:layout_constraintEnd_toStartOf="@+id/t"

        android:textSize="20dp"
        />
    <TextView
        android:id="@+id/t"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:background="#004740"
        android:gravity="center"
        android:text="Account"
        android:textFontWeight="1"
        android:textStyle="bold"
        android:textColor="#96DCD5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/s"
        app:layout_constraintEnd_toEndOf="parent"

        android:textSize="20dp"
        />





</androidx.constraintlayout.widget.ConstraintLayout>